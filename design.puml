        H_INVOKEVIRTUAL: int
        INVOKEDYNAMIC: int
        INVOKEINTERFACE: int
        INVOKESPECIAL: int
        INVOKEVIRTUAL: int
        ALOAD: int
        DLOAD: int
        FLOAD: int
        ILOAD: int
        LLOAD: int
        ASTORE: int
        DSTORE: int
        FSTORE: int
        ISTORE: int
        LSTORE: int
        ASM8: int
    }
}

