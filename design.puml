        }

        +class MyASMMethodInsnNode{
            -{final}methodInsnNode: MethodInsnNode
            -{final}factory: MyASMAbstractInsnNodeFactory
            +MyASMMethodInsnNode(methodInsNode: AbstractInsnNode)
