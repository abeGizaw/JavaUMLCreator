            -convertLocalVariableNodes(): List<MyLocalVariableNode>
        }

        +class MyASMType {
            -{final}type: Type
            +MyASM(type: Type)
            +MyASM()
            +getArgumentTypes(): Type[]
            +getType(): MyType
        }

        +class MyASMVarInsnNode {
            -{final}varInsnNode: VarInsnNode
            +MyASMVarInsnNode(varInsnNode: VarInsnNode)
        }
