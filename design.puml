            +getOpcode(): int
            +getType(): int
        }

        +class MyASMClassReader{
            -classReader: ClassReader

            +MyASMClassReader(classReader: ClassReader)
        }

        +class MyASMFieldNode {
            -{final}fieldNode: FieldNode
            +MyASMFieldNode(fieldNode: FieldNode)
        }

        +class MyASMLabel{
            -{final}label: Label
